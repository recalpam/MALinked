[31m-;(function () {[m
[31m-	'use strict';[m
[31m-[m
[31m-	/**[m
[31m-	 * @preserve FastClick: polyfill to remove click delays on browsers with touch UIs.[m
[31m-	 *[m
[31m-	 * @codingstandard ftlabs-jsv2[m
[31m-	 * @copyright The Financial Times Limited [All Rights Reserved][m
[31m-	 * @license MIT License (see LICENSE.txt)[m
[31m-	 */[m
[31m-[m
[31m-	/*jslint browser:true, node:true*/[m
[31m-	/*global define, Event, Node*/[m
[31m-[m
[31m-[m
[31m-	/**[m
[31m-	 * Instantiate fast-clicking listeners on the specified layer.[m
[31m-	 *[m
[31m-	 * @constructor[m
[31m-	 * @param {Element} layer The layer to listen on[m
[31m-	 * @param {Object} [options={}] The options to override the defaults[m
[31m-	 */[m
[31m-	function FastClick(layer, options) {[m
[31m-		var oldOnClick;[m
[31m-[m
[31m-		options = options || {};[m
[31m-[m
[31m-		/**[m
[31m-		 * Whether a click is currently being tracked.[m
[31m-		 *[m
[31m-		 * @type boolean[m
[31m-		 */[m
[31m-		this.trackingClick = false;[m
[31m-[m
